
  <h1>Browser</h1>
  <form action="/" method="get">
    <input type="text" class="search" name="search" value="{{search}}" placeholder="Search...">
    <input type="number" name="from" class="difficulty" placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" name="to" class="difficulty" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>

  {{#if allCubes}}
    {{#each allCubes}}
      {{>cubeCard}}
    {{/each}}

    {{else}}
    <div class="noItems">
      <p>Nothing has been listed yet. Be the first!</p>
      <a href="/create">Create a new cube</a>
    </div>
  {{/if}}
